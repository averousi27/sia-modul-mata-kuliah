package com.AIS.Modul.MataKuliah.Repository;

import java.util.List;
import java.util.UUID;

import com.sia.main.domain.EkuivalensiMK;

public interface EkuivalensiMKRepository {

	long count(String where);

	List<EkuivalensiMK> get(String where, String order, int limit, int offset);

	void update(EkuivalensiMK ekuivalensiMK);

	UUID insert(EkuivalensiMK ekuivalensiMK);

	EkuivalensiMK findById(UUID idEkuivalensiMK);

	List<EkuivalensiMK> findAll();

}
