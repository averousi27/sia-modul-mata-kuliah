	List<EkuivalensiMK> get(String where, String order);

	List<EkuivalensiMK> get(String where, String order, int limit, int offset);

	String save(EkuivalensiMK ekuivalensiMK);

	EkuivalensiMK findById(UUID idEkuivalensiMK);

	String delete(UUID idEkuivalensiMK);

