package com.AIS.Modul.MataKuliah.Repository;

import java.util.List;
import java.util.UUID;

import com.sia.main.domain.CapPemb;
import com.sia.main.domain.SubCapPemb;

public interface SubCapPembRepository {

	long count(String where);

	List<SubCapPemb> get(String where, String order, int limit, int offset);

	void update(SubCapPemb subCapPemb);

	UUID insert(SubCapPemb subCapPemb);

	SubCapPemb findById(UUID idSubCapPemb);

	List<SubCapPemb> findAll();

}
